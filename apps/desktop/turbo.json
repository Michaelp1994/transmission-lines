{
    "$schema": "https://turbo.build/schema.json",
    "extends": ["//"],
    "pipeline": {
        "build": {
            "dependsOn": ["^build"],
            "outputs": ["dist/**"]
        },
        "dev": {
            "dependsOn": ["^build", "setup-dev"]
        },
        "test": {
            "dependsOn": ["^build", "build"]
        },
        "setup-dev": {
            "cache": false
        }
    }
}
