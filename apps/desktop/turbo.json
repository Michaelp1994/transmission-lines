{
  "$schema": "https://turbo.build/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "dev": {
      "dependsOn": [
        "setup-dev"
      ]
    },
    "test": {
      "dependsOn": [
        "^build",
        "build"
      ]
    },
    "setup-dev": {
      "cache": false
    }
  }
}
