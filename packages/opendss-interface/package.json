{
    "name": "@repo/opendss-interface",
    "type": "module",
    "exports": {
        ".": "./dist/index.js",
        "./classes/*": "./dist/classes/*.js",
        "./elements/*": "./dist/classes/elements/*.js",
        "./elements": "./dist/classes/elements/index.js",
        "./elements/**/*": "./dist/classes/elements/**/*.js"
    },
    "scripts": {
        "build": "rm -rf dist && tsc && tsc-alias",
        "clean": "rm -rf .turbo node_modules dist",
        "lint": "eslint src/",
        "test": "jest && echo \"finished\"",
        "typecheck": "tsc --noEmit",
        "watch": "tsc --watch"
    },
    "dependencies": {
        "winax": "^3.4.2"
    },
    "devDependencies": {
        "@repo/eslint-config": "workspace:*",
        "@types/activex-opendssengine": "^4.0.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.2",
        "tsc-alias": "^1.8.8",
        "vite-tsconfig-paths": "^4.3.1",
        "vitest": "^1.2.2"
    },
    "packageManager": "yarn@4.1.0"
}
