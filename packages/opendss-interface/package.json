{
    "name": "@repo/opendss-interface",
    "type": "module",
    "exports": {
        ".": "./src/index.ts",
        "./classes/*": "./src/classes/*.ts",
        "./elements/*": "./src/classes/elements/*.ts",
        "./elements": "./src/classes/elements/index.ts",
        "./elements/**/*": "./src/classes/elements/**/*.ts"
    },
    "scripts": {
        "build": "tsc && tsc-alias",
        "clean": "rm -rf .turbo node_modules dist",
        "lint": "eslint src/",
        "lint:fix": "eslint src/ --fix",
        "test": "jest",
        "typecheck": "tsc --noEmit",
        "watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")"
    },
    "devDependencies": {
        "@jest/globals": "^29.7.0",
        "@repo/db": "workspace:*",
        "@repo/eslint-config": "workspace:*",
        "@repo/typescript-config": "workspace:*",
        "@types/activex-opendssengine": "^4.0.0",
        "concurrently": "^8.2.2",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.2",
        "tsc-alias": "^1.8.8",
        "typescript": "^5.3.3",
        "vite-tsconfig-paths": "^4.3.1",
        "vitest": "^1.2.2",
        "winax": "3.4.2"
    },
    "peerDependencies": {
        "winax": "3.4.2"
    }
}
