{
    "name": "@repo/logger",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "exports": {
        ".": {
            "require": "./dist/index.cjs",
            "import": "./dist/index.js"
        }
    },
    "types": "./dist/index.d.ts",
    "files": [
        "dist/**"
    ],
    "scripts": {
        "build": "tsc && tsc-alias",
        "clean": "rm -rf .turbo node_modules dist",
        "lint": "eslint src/",
        "test": "jest ",
        "typecheck": "tsc --noEmit",
        "watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")"
    },
    "devDependencies": {
        "@repo/eslint-config": "workspace:*",
        "@repo/jest-presets": "workspace:*",
        "@repo/typescript-config": "workspace:*",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.16",
        "concurrently": "^8.2.2",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "tsc-alias": "^1.8.8",
        "typescript": "^5.3.3"
    }
}
